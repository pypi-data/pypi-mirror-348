# Finance News API - Environment Variables
# Copy this file to .env and fill in the values

# API Authentication
API_KEY=your_api_key_here
# Database Configuration
# Uncomment and set this if you want to use a custom database location
# DATABASE_URL=sqlite:///path/to/your/custom/database.db
# Debug Mode
# Set to True for development, False for production
DEBUG=False
# Logging Level
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
