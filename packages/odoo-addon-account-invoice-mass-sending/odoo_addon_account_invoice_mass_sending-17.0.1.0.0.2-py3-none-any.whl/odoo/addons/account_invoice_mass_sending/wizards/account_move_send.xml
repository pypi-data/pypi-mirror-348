<odoo>
    <record id="account_move_send_wizard_form" model="ir.ui.view">
        <field name="model">account.move.send</field>
        <field name="inherit_id" ref="account.account_move_send_form" />
        <field name="arch" type="xml">
            <button name="action_send_and_print" position="before">
                <button
                    name="enqueue_invoices"
                    string="Email mass sending (Job Queue)"
                    type="object"
                    invisible="not checkbox_send_mail or not mail_template_id"
                    class="btn-primary"
                />
            </button>
        </field>
    </record>
</odoo>
