  "bin": {
    "mcp-server-pyautogui": "dist/index.stdio.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "src",
    ".python-version",
    "pyproject.toml",
    "uv.lock"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
