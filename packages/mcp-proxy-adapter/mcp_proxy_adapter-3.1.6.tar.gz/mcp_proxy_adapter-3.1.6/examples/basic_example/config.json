{
  "service": {
    "name": "Basic Example",
    "version": "1.0.0"
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": true,
    "log_level": "info"
  },
  "logging": {
    "level": "INFO",
    "file": "logs/basic_example.log",
    "rotation": {
      "type": "size",
      "max_bytes": 10485760,
      "backup_count": 5
    }
  },
  "discovery": {
    "enabled": true,
    "package": "commands"
  }
} 