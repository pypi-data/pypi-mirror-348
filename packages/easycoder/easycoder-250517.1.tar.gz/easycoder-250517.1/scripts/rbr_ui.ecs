
!   rbr-ui.ecs

    script RBR_UI

    use graphics
    
    import plugin RBR_UI from plugins/rbr_ui.py

    window Window
    rbr_ui MainPanel

    debug step

    init graphics

    create Window title `Room By Room` size 600 1024

    create MainPanel in Window
!    set the layout of Window to MainPanel
    show Window

    start graphics
    stop